{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "newVMName": {
      "type": "string",
      "defaultValue": "IFWin10gold"
    },
    "labName": {
      "type": "string",
      "defaultValue": "DDITImageFactory"
    },
    "size": {
      "type": "string",
      "defaultValue": "Standard_F16s"
    },
    "userName": {
      "type": "string",
      "defaultValue": "VMAdmin"
    },
    "password": {
      "type": "securestring"
    }
  },
  "variables": {
    "vmId": "[resourceId ('Microsoft.DevTestLab/labs/virtualMachines', parameters('labName'), parameters('newVMName'))]",
    "vmName": "[concat(parameters('labName'), '/', parameters('newVMName'))]"
  },
  "resources": [
    {
      "apiVersion": "2016-05-15",
      "type": "Microsoft.DevTestLab/labs/virtualMachines",
      "name": "[variables('vmName')]",
      "location": "[resourceGroup().location]",
      "properties": {
        "labVirtualNetworkId": "[resourceId('Microsoft.DevTestLab/labs/virtualnetworks', parameters('labName'), 'imagefactory-corp-wus2-vnet-1')]",
        "notes": "W10T-x64",
        "customImageId": "/subscriptions/6fd2cc40-ad0c-4f8f-a1c4-9ab4e656c849/resourcegroups/dditimagefactoryrg886736/providers/microsoft.devtestlab/labs/dditimagefactory/customimages/w10t-x64-clean",
        "size": "[parameters('size')]",
        "userName": "[parameters('userName')]",
        "password": "[parameters('password')]",
        "isAuthenticationWithSshKey": false,
        "artifacts": [
          {
            "artifactId": "[resourceId('Microsoft.DevTestLab/labs/artifactSources/artifacts', parameters('labName'), 'Public Repo', 'windows-enable-local-admins')]"
          },
          {
            "artifactId": "[resourceId('Microsoft.DevTestLab/labs/artifactSources/artifacts', parameters('labName'), 'privaterepo334', 'windows-install-oneget')]"
          },
          {
            "artifactId": "[resourceId('Microsoft.DevTestLab/labs/artifactSources/artifacts', parameters('labName'), 'privaterepo334', 'windows-updates')]"
          },
          {
            "artifactId": "[resourceId('Microsoft.DevTestLab/labs/artifactSources/artifacts', parameters('labName'), 'privaterepo334', 'windows-enableadminaftersysprep')]"
          }
        ],
        "labSubnetName": "CorpNet",
        "disallowPublicIpAddress": true
      }
    }
  ],
  "outputs": {
    "labVMId": {
      "type": "string",
      "value": "[variables('vmId')]"
    }
  }
}
